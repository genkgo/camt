parameters:
    level: max
    paths:
        - src/
        - test/
    inferPrivatePropertyTypeFromConstructor: true
    checkMissingIterableValueType: false
    ignoreErrors:
        -
            message: '~^Cannot call method .*\(\) on Genkgo\\Camt\\.*\|null\.$~'
            path: test
        -
            message: '~^Cannot call method format\(\) on DateTimeImmutable\|null\.$~'
            path: test
